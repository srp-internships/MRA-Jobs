{
  "UseInMemoryDatabase": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "MRA.Jobs.Web": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*",
  "Sieve": {
    "CaseSensitive": false,
    "DefaultPageSize": 20,
    "MaxPageSize": 500,
    "ThrowExceptions": true,
    "IgnoreNullsOnNotEqual": true,
    "DisableNullableTypeExpressionForSorting": false
  },
  "TwilioSettings": {
    "AccountSid": "",
    "AuthToken": "",
    "FromNumber": ""
  },
  "SmsSettings" : {
  }
}
